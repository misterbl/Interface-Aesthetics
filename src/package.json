{
  "name": "interface-aesthetics",
  "description": "interface-aesthetics",
  "scripts": {
    "createfirebaseconf": "firebase setup:web --json > ../functions/firebase-config.json",
    "buildClient": "./node_modules/.bin/webpack --config ./webpack.client.config.js",
    "buildServer": "./node_modules/.bin/webpack --config ./webpack.server.config.js",
    "build": "npm run createfirebaseconf && npm run buildClient && npm run buildServer",
    "watch": "watch 'npm run buildClient'",
    "serve": "firebase serve --only hosting,functions"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-client": "^2.5.1",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-plugin-file-loader": "^1.1.1",
    "css-loader": "^2.1.1",
    "draft-js": "^0.10.5",
    "file-loader": "^3.0.1",
    "firebase": "5.8.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "lodash": "4.17.4",
    "react": "^16.7.0",
    "react-apollo": "^2.5.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.7.0",
    "react-modal": "^3.8.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "4.5.2",
    "null-loader": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "webpack": "4.19.1",
    "webpack-cli": "^3.2.3"
  },
  "babel": {
    "presets": [
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-class-properties",
      [
        "file-loader",
        {
          "name": "[hash].[ext]",
          "extensions": [
            "png",
            "jpg",
            "jpeg",
            "gif",
            "svg"
          ],
          "outputPath": "../public",
          "publicPath": "/",
          "context": "",
          "limit": 0
        }
      ]
    ]
  }
}
